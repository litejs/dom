{
  "name": "dom-lite",
  "version": "20.2.0",
  "license": "MIT",
  "author": "Lauri Rooden <lauri@rooden.ee>",
  "description": "A small DOM implementation",
  "keywords": [
    "browser",
    "document",
    "dom",
    "litejs"
  ],
  "main": "index.js",
  "readmeFilename": "README.md",
  "files": [
    "index.js"
  ],
  "repository": "git://github.com/litejs/dom-lite.git",
  "bugs": "https://github.com/litejs/dom-lite/issues",
  "dependencies": {
    "selector-lite": "20.2.0"
  },
  "litejs": {
    "lint": "jshint *.js"
  },
  "jshintConfig": {
    "asi": true,
    "laxcomma": true,
    "maxdepth": 6,
    "quotmark": "double"
  },
  "nyc": {
    "check-coverage": true,
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100,
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  }
}

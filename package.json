{
  "name": "@litejs/dom",
  "version": "23.2.1",
  "description": "A small DOM library for server-side testing, rendering, and handling of HTML files",
  "license": "MIT",
  "author": "Lauri Rooden <lauri@rooden.ee>",
  "keywords": [
    "document",
    "dom",
    "DOMParser",
    "XMLHttpRequest",
    "XMLSerializer",
    "litejs"
  ],
  "files": [
    "*.js"
  ],
  "scripts": {
    "test": "lj test --coverage --ignore=xmlhttprequest.js --brief test/index.js && jshint *.js"
  },
  "repository": "github:litejs/dom",
  "devDependencies": {
    "@litejs/cli": "23.2.6",
    "jshint": "2.13.6"
  },
  "litejs": {
    "build": false,
    "global": false
  },
  "jshintConfig": {
    "esversion": 5,
    "asi": true,
    "evil": true,
    "laxcomma": true,
    "maxdepth": 6,
    "node": true,
    "nonbsp": true,
    "undef": true,
    "unused": true,
    "shadow": "outer",
    "quotmark": "double"
  }
}
